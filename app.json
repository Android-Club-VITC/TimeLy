{
  "expo": {
    "name": "TimeLy",
    "slug": "TimeLy",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.androidclub.timely",
      "buildNumber": "1.2.0",
      "icon": "./assets/ios-icon.png"
    },
    "android": {
      "icon": "./assets/icon.png",
      "package": "com.androidclub.timely",
      "permissions": ["RECEIVE_BOOT_COMPLETED"],
      "versionCode": 1,
      "useNextNotificationsApi": true
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "mode": "production"
        }
      ]
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
